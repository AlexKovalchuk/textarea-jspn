{
    "defaultSeverity": "error",
    "extends": [
        "tslint-react",
        "tslint:recommended"
    ],
    "jsRules": {
    },
    "rules": {
        "member-access": false,
        "ordered-imports": false,
        "quotemark": false,
        "no-console": false,
        "semicolon": false,
        "jsx-no-multiline-js": false,
        "exhaustive-deps": false,
        "jsx-no-lambda": false,
        "no-any": true
    },
    "rulesDirectory": [
    ],
    "linterOptions": {
        "exclude": [
            "config/**/*.js",
            "node_modules/**/*.ts"
       ]
   }
}